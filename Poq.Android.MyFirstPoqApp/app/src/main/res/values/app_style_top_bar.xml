<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<resources>
    <style name="font_style_label_top_bar" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">17sp</item>
        <item name="android:fontFamily">@font/system_Semibold</item>
    </style>
    <color name="color_label_top_bar">#FF080808</color>
    <style name="label_top_bar" parent="font_style_label_top_bar">
        <item name="android:textColor">@color/color_label_top_bar</item>
    </style>
    <color name="color_top_bar">#FFFFFFFF</color>
    <style name="top_bar_theme">
        <item name="android:actionMenuTextColor">@color/color_label_top_bar</item>
    </style>
    <style name="top_bar">
        <item name="android:background">@color/color_top_bar</item>
        <item name="titleTextAppearance">@style/label_top_bar</item>
        <item name="android:theme">@style/top_bar_theme</item>
    </style>
</resources>
